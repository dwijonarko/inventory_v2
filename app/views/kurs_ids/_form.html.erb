<div id="dialog_form">
  <% semantic_form_for @kurs_id, {}, :html => { :id => 'kurs_id_form' } do |f| %>
    <ul class="form_info">
      <li>Fields marked <span class="required">*</span> are required</li>
    </ul>
    <% f.inputs do %>
      <%= f.input :code %>
      <%= f.input :name %>
      <%= f.input :symbol %>
    <% end -%>
    <fieldset class="buttons">
      <button type="submit"><%= image_tag('icons/silk/disk.png') %> Save Kurs ID</button>
    </fieldset>
  <% end %>
</div>
