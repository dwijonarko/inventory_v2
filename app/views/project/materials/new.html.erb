<% semantic_form_for @material, :url => project_spk_materials_path do |f| %>
  <%= f.error_messages %>
  <% f.inputs "Material" do %>
    <li class="string">
      <%= f.label :item_name, "Item" %>
      <div class="inline">
        <%= f.text_field :item_name, :class => "item_autocomplete " %>
      </div>
      <div class="inline">
        <%= f.text_field :price %>
      </div>
      <div class="inline">
        <%= f.text_field :quantity %>
      </div>
    </li>
  <% end %>
<% end %>
